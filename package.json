    "react-hot-toast": "^2.5.2",
    "react-paginate": "^8.3.0",
    "react-select": "^5.9.0",
    "react-switch": "^7.1.0",
    "react-to-print": "^3.0.3",
    "react-toastify": "^11.0.5",
    "terser-webpack-plugin": "^5.3.11",
    "to-words": "^4.6.0",
